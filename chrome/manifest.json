{
  "manifest_version": 2,
  "name": "Intelli Octo",
  "version": "0.0.1",
  "description": "Intelli Octo",
  "homepage_url": "https://github.com/pd4d10/intelli-octo",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "github.bundle.js"
      ],
      "run-at": "document_idle"
    },
    {
      "matches": [
        "https://gitlab.com/*"
      ],
      "js": [
        "gitlab.bundle.js"
      ],
      "run-at": "document_idle"
    }
  ]
}
