{
  "manifest_version": 2,
  "name": "Octohint",
  "version": "1.6.0",
  "description": "IntelliSense hint for GitHub",
  "homepage_url": "https://github.com/pd4d10/octohint",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "dist/sentry.js",
      "dist/ts-lib.js",
      "dist/background.js"
    ]
  },
  "permissions": [
    "https://github.com/*",
    "https://gist.github.com/*",
    "https://gitlab.com/*",
    "https://bitbucket.org/*"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "options_page": "dist/options.html",
  "options_ui": {
    "page": "dist/options.html",
    "chrome_style": true
  }
}
