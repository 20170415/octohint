{
  "name": "intelli-octo",
  "private": true,
  "version": "1.3.0",
  "description": "IntelliSense for GitHub, GitLab and Bitbucket",
  "scripts": {
    "start": "webpack -w",
    "build": "webpack --config webpack.config.production.js",
    "package-chrome": "cd chrome/; cp ../LICENSE ./; zip -r ../chrome.zip ./; rm LICENSE; cd ../",
    "package-safari": "cd intelli-octo.safariextension/; cp ../LICENSE ./; zip -r ../intelli-octo.safariextension.zip ./; rm LICENSE; cd ../",
    "package": "npm run package-chrome && npm run package-safari",
    "test": "tslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pd4d10/intelli-octo.git"
  },
  "keywords": [],
  "author": "pd4d10 <pd4d10@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pd4d10/intelli-octo/issues"
  },
  "homepage": "https://github.com/pd4d10/intelli-octo",
  "dependencies": {
    "@types/chrome": "^0.0.46",
    "@types/node": "^7.0.12",
    "@types/safari-extension": "^0.0.27",
    "@types/safari-extension-content": "^0.0.14",
    "github-injection": "^0.3.0",
    "lodash": "^4.17.4",
    "preact": "^8.1.0",
    "raven-js": "^3.14.0",
    "typescript": "^2.3.2",
    "vscode-css-languageservice": "^2.0.0",
    "vscode-html-languageservice": "^2.0.1",
    "vscode-json-languageservice": "^2.0.7",
    "vscode-languageserver-types": "^3.2.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.2.1",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.17.0",
    "tslint": "^5.1.0",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-merge": "^4.1.0"
  }
}
